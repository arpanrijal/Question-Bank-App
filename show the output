// rewrite the following program by correcting any errors, if present and also write down the output of the corrected code?    [2074 Ashwin]

Define MAX'S'

int main()

{

    int case[MAX]={2,3,5,4,10},i,sum=0;

    for(i=0,i<MAX,i+=1)

       {

           printf("case %d=%3.2d\n",I,case[i]);

           sum+=*case+i;

       };

     average=sum/MAX;

    printf("%06.2f",average);

     return 1;

}



