//palindrome Fibonacci Armstrong questions and solution function

void paliFiboArm()
{
    char[3] opt,armopt;
    int num,n1,n2,numarm,z=0,count=0,temp;
start:
    clrscr();
    printf("Questions");
palfibarmhome:
    clrs();
    printf("\n\nFor source code enter \"s\" for code execution enter \"exe\": ");
    scanf("%s",&opt);
    switch(opt)
    {
    case 's':
        printf("Questions");
        printf("\n\nPress \"c\" to continue, press \"b\" to go to home screen and press \"e\" to exit the program: ");
        scanf("%c",&decision);
        if(decision=='c')
        {
            goto selectyear;
        }
        if(decision=='e')
        {
            clrscr();
            printf("Exiting....");
            exit(0);
        }
        if(decision=='b')
        {
            clrscr();
            main();
            break;
        }

    case 'exe':
        printf("Questions");
        printf("\n\nEnter question number of that code: ");
        scanf("%d",&num);
        if(num==1)
        {
            printf("To generate fibonacci series less that 500\n\n");
            printf("Enter two number: ");
            scanf("%d%d",&n1,&n2);
            while(series<500)
            {
                series=n1+n2;
                if(series<500)
                {
                    printf("%d, ",series);
                    n1=n2;
                    n2=series;
                }
                else
                {
                    break;
                }
            }
            printf("\n\n\Enter \"m\" for this section main menu or \"h\" for homescreen\n enter any character to exit the program: ");
            scanf("%c",armopt);
            if(armopt=='m')
            {
                goto start;
            }
            else if(armopt=='h')
            {
                main();
            }
            else
            {
                exit(0);
            }
            break;


        }
        else if(num==2)
        {
            printf("To check the given number is Armstrong number or not\n\n");
            printf("Enter number: ");
            scanf("%d",numarm);
            temp=numarm;
            while(temp!=0)
            {
                temp=temp/10;
                count++;
            }
            temp1=numarm;
            while(temp1!=0)
            {
                rem=temp1%10;
                z=z+pow(rem,count);
                temp1=temp1/10;
            }
            if(z==numarm)
            {
                printf("%d is Armstrong number!",z);
            }
            else
            {
                printf("%d isn't Armstrong number!",z);
            }
            printf("\n\n\Enter \"m\" for this section main menu or \"h\" for homescreen\n enter any character to exit the program: ");
            scanf("%c",armopt);
            if(armopt=='m')
            {
                goto start;
            }
            else if(armopt=='h')
            {
                main();
            }
            else
            {
                exit(0);
            }
            break;
        }
        else if(num==3)
        {

//remaining is coming soon!
        }
    default:


        printf("\n\nInvalid Input");
        goto palfibarmhome;
        break;



    }




}

// function of show output of a program

void showOutputQuest()
{

    char opt1;
    int sourCod,I,j,k;
topOfCode:
    printf("Questions");
    printf("\n\nFor source code enter \"s\" for code execution enter \"e\": ");
    scanf("%s",&opt1);
    switch(opt1)
    {
    case 's':
        clrscr();
        printf("source code question show");
        printf("Enter the number of which you want to see: ");
        scanf("%d",&sourCod);//total source code is 12
        if(sourCod==1)
        {

        }
        break;
    case 'e':
        clrscr();
        printf("source code question show");
startshowoutput:
        printf("Enter the number of which you want to see: ");
        scanf("%d",&sourCod);//total source code is 12
        if(sourCod==1)
        {

        }
        else if(sourCod==2)
        {

        }
        else if(sourCod==3)
        {

        }
        else if(sourCod==4)
        {

        }
        else if(sourCod==5)
        {

        }
        else if(sourCod==6)
        {

        }
        else if(sourCod==7)
        {

        }
        else if(sourCod==8)
        {

        }
        else if(sourCod==9)
        {

        }
        else if(sourCod==10)
        {

        }
        else if(sourCod==11)
        {

        }
        else if(sourCod==12)
        {

        }
        else
        {
            printf("Invalid Input please enter num between 1-12!");
            goto startshowoutput;
        }
        break;
    default:
        printf("Enter \"e\" for execution of code and \"s\" for source code");
        goto topOfCode;

    }

}





