    printf("\n5. \"Running consol\" of past year question (2069-2080)\n");
    printf("\n6. \"Show the output\" type question solution (2069-2080)\n");
wrongInput:
    printf("\n\nEnter the category number to go to that section: ");
    scanf("%d",&choice);
    switch(choice)
    {
    case 1:
        clrscr();
        quesPast();
        break;

    case 2:
        clrscr();
        printf("2");
        //  repeatQuestPastYear();
